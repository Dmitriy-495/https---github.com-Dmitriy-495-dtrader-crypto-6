{
  "name": "trading-bot-v6",
  "version": "6.0.0",
  "description": "Advanced algorithmic trading bot for Gate.io with TypeScript",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "ts-node-dev --respawn --transpile-only index.ts",
    "build": "tsc",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "clean": "rimraf dist",
    "prepare": "npm run build"
  },
  "keywords": [
    "trading",
    "bot",
    "cryptocurrency",
    "gateio",
    "typescript",
    "algorithmic"
  ],
  "author": "Trading Bot Team",
  "license": "MIT",
  "dependencies": {
    "terminal-kit": "^3.0.1",
    "axios": "^1.7.9",
    "ws": "^8.18.0",
    "config": "^3.3.12",
    "dotenv": "^16.4.7",
    "winston": "^3.17.0",
    "lodash": "^4.17.21",
    "dayjs": "^1.11.18",
    "ccxt": "^4.5.4",
    "eventemitter3": "^5.0.1"
  },
  "devDependencies": {
    "@types/node": "^22.10.5",
    "@types/ws": "^8.5.12",
    "@types/lodash": "^4.17.20",
    "@types/config": "^3.3.5",
    "@typescript-eslint/eslint-plugin": "^8.44.0",
    "@typescript-eslint/parser": "^8.44.0",
    "eslint": "^8.57.1",
    "jest": "^29.7.0",
    "@types/jest": "^29.5.14",
    "ts-jest": "^29.2.6",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.7.3",
    "rimraf": "^6.0.1",
    "prettier": "^3.4.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
